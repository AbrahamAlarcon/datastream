# Weather Rest connection
weather.url: http://api.wunderground.com/api/${weather.key}
weather.retry.policy.maxAttempts: 3
weather.retry.policy.backOffPeriodMillis: 5000
hystrix.command.weather.execution.isolation.thread.timeoutInMilliseconds: 2000