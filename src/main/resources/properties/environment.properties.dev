# Logging Thread Pool Task Executor
logging.threadPoolTaskExecutor.threadNamePrefix: Logging-
logging.threadPoolTaskExecutor.corePoolSize: 2
logging.threadPoolTaskExecutor.maxPoolSize: 2
logging.threadPoolTaskExecutor.queueCapacity: 500

